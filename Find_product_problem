import java.util.*;
public class TestClass
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		long b=1;
		int c[]= new int[a];
		int mod=1000000000+7;

		for(int i=0;i<c.length;i++)
		{
		    c[i]=sc.nextInt();
		    b=(b*c[i])%mod;
		}
		System.out.println(b);
	}
}
